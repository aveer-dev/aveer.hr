{
	"[typescript]": {
		"editor.defaultFormatter": "denoland.vscode-deno"
	},
	"deno.lint": true,
	"scss.lint.unknownAtRules": "ignore",
	"search.exclude": {
		"**/node_modules": false
	},
	"deno.importMap": "./import_map.json",
	"deno.enable": true,
	"deno.unstable": ["bare-node-builtins", "byonm", "sloppy-imports", "unsafe-proto", "webgpu", "broadcast-channel", "worker-options", "cron", "kv", "ffi", "fs", "http", "net"]
}
